{
    "name": "Save tabs",
    "version": "1.0",
    "description": "Store tabs",
    "permissions": [
        "storage",
        "tabs",
        "unlimitedStorage"
    ],
    "background": {
        "page": "background.html",
        "persistent": false
    },
    "browser_action": {
        "default_icon": "./images/tabsIcon.png"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net"
}